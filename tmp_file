<script>
  jQuery.post("/.ajax/website",{action: "getUsername"}).always(function(data) 
  {
    if (data.username) 
    {
       jQuery(".account-link").html(data.username);
       jQuery(".register-logout-link")
          .html("logout")
          .attr("href","https://sso.bitwig.com/logout?service=https://www.bitwig.com/en?mgnlLogout");
    }
    else
    {
       jQuery(".account-link").html("Login");
       jQuery(".register-logout-link")
          .html("Create Account")
          .attr("href","/en/account/register.html");
    }
  });

  $('#show-global-nav-menu').click(function() {
    var $nm = $('#nav-global-mini');
    var $mh = $("#meta-header");
    $nm.toggle();
    
    if ($nm.css('display') !== 'none')
    {
      $("body").css('overflowY', 'hidden');
      $mh.css('maxHeight', $(window).height() + "px"); 
      $mh.css('overflowY', "scroll"); 
    }
    else
    {
      $("body").css('overflowY', 'auto');
      $mh.css('overflowY', "auto"); 
    }
  });
  
  $(window).scroll(function() {
    var $logo = $('#logo');

    if ($(this).scrollTop() > 35)
    {
      $('header').addClass("sticky");
      var currSrc = $logo.attr("src");
      var altSrc = $logo.data("altSrc");
      
      if (altSrc && (altSrc != currSrc))
      {
        $logo.fadeOut("fast", function() { $logo.attr("src", altSrc).fadeIn("fast"); });
      }
    }
    else
    {
      $('header').removeClass("sticky");
      var currSrc = $logo.attr("src");
      var origSrc = $logo.data("origSrc");
      
      if (origSrc && (origSrc != currSrc))
      {
        $logo.fadeOut("fast", function() { $logo.attr("src", origSrc).fadeIn("fast"); });
      }
    }
  });
</script>

