  <script>
    $("footer .footer-bar").clone().attr("id","sticky-footer").addClass("sticky").hide().insertBefore("footer");
  
    $(window).scroll(function()
    {
      updateStickyFooter();
    });
    
    function updateStickyFooter()
    {
      if ($(window).scrollTop() + $(window).height() > $("body").height() - $("footer").height())
      {
        $("#sticky-footer").hide();
      }
      else
      {
        $("#sticky-footer").show();
      }
    }
    
    $(function() {
      $f = $("footer");
      $t = $f.offset().top;
      $wh = $(window).height();
      
      if ($t < $wh)
      {
        $("<div/>").css({"width" : "1px", "height" : ($wh - $t) + "px"}).insertBefore($("#footer-wrapper"));
      }
      
      updateStickyFooter();
    });

    $(document).ajaxComplete(function() {
      updateStickyFooter();
    });
  </script>
